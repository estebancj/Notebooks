    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#TipoDeDocumentos -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#TipoDeDocumentos">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#DatosRepositorioInstitucional"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RepositorioInstitucional"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="es">Descripción de los documentos electrónicos almacenados en el repositorio.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#TipoPolitica -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#TipoPolitica">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#DatosPolitica"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#PoliticaRepositorio"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="es">Detalles del propósito de las acciones a seguir para asegurar el acceso/administración del repositorio.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#TipoReporte -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#TipoReporte">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#DatosReporteInvestigacion"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ReporteDeInvestigacion"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="es">Detalles acerca del área y tópico asociados al contenido del reporte escrito.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Titulo -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Titulo">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#DatosDocumento"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RecursoDeInformacion"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="es">Nombre usado para identificar un documento electrónico.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Volumen -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Volumen">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#DatosDocumento"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ArticuloCientifico"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ArticuloDivulgacion"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#LibroCientifico"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#LibroDivulgacion"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment xml:lang="es">Parte o apartado conceptual en que se estructura la publicación periodica de revistas de investigación y divulgación.</rdfs:comment>
    </owl:DatatypeProperty>
    


