     -->

    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#API -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#API">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#GlosarioTerminos"/>
        <rdfs:comment xml:lang="en">Application Programming Interface</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">Interfaz para Programación de Aplicaciones, por sus siglas en inglés.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Academico -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Academico">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Persona"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Alumno"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Discente"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Estudiante"/>
        <rdfs:comment xml:lang="es">Persona asociada al ámbito de la enseñanza en distintos niveles de la educación.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Acceso -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Acceso">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#PoliticaRepositorio"/>
        <rdfs:isDefinedBy xml:lang="es">Conjunto de objetivos, estrategias y líneas de acción que buscan definir los derechos y mecanismos de consulta de los usuarios potenciales del repositorio institucional.</rdfs:isDefinedBy>
        <rdfs:seeAlso rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#AccesoAbierto"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#AccesoARecursosDeInformacionAcademicaCientificaTecnologicaYDeInnovacion -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#AccesoARecursosDeInformacionAcademicaCientificaTecnologicaYDeInnovacion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#AccesoAbierto"/>
        <rdfs:comment xml:lang="es">El acceso al que se hace referencia también incluye bases de datos que contienen los registros de citas e información bibliográfica de artículos de revistas científicas, tecnológicas y de innovación, tesis y disertaciones, protocolos, memorias de congresos y patentes, entre otros. Asimismo, se contemplan colecciones de conocimientos accesibles de los productos de investigación como informes y reportes, bases de datos primarias de investigación.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="es">Conjunto de técnicas utilizadas para buscar, categorizar y acceder de manera inequívoca, al texto completo de publicaciones reconocidas por los sectores académico, de ciencia, tecnología e innovación. El acceso al que se hace referencia también incluye bases de datos que contienen los registros de citas e información bibliográfica de artículos de revistas científicas, tecnológicas y de innovación, tesis y disertaciones, protocolos, memorias de congresos y patentes, entre otros. Asimismo se contemplan colecciones de conocimientos accesibles de los productos de investigación como informes y reportes, bases de datos primarias de investigación.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#AccesoAbierto -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#AccesoAbierto">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RepositorioInstitucional"/>
        <rdfs:comment xml:lang="es">En este tipo de acceso, cualquier persona con conexión a internet, puede tener acceso sin restricciones económicas, técnicas o legales, a información científica, académica, de desarrollo tecnológico, innovación o cultural producida con recursos públicos o que en su producción se usó infraestructura pública.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="es">Acceso a través de una plataforma digital y sin requerimientos de suscripción, registro o pago, a las investigaciones, materiales educativos, académicos, científicos, tecnológicos y de innovación, financiados total o parcialmente con recursos públicos o que hayan utilizado infraestructura pública en su realización, sin perjuicio de las disposiciones en materia de patentes, protección de la propiedad intelectual o industrial, seguridad nacional y derechos de autor, entre otras, así como de aquella información que, por razón de su naturaleza o decisión del autor, sea confidencial o reservada.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Administrador -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Administrador">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Persona"/>
        <rdfs:isDefinedBy xml:lang="es">Responsable de controlar y/o supervisar el buen funcionamiento del repositorio institucional.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Administrativo -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Administrativo">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Persona"/>
        <rdfs:comment xml:lang="es">Persona encargada del funcionamiento, el rendimiento y la estructura de una organización.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Almacenamiento -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Almacenamiento">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Funcionalidad"/>
        <rdfs:comment xml:lang="es">Proceso de acopio de recursos de información en el repositorio institucional.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Almetria -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Almetria">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#GlosarioTerminos"/>
        <rdfs:comment xml:lang="es">Además del número de citas, las métricas contemplan otros aspectos del impacto de un trabajo como bases de datos que se refieren a él, visualizaciones, descargas, menciones en redes sociales o en prensa.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="es">Conjunto de métricas alternativas al factor de impacto.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Alumno -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Alumno">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Estudiante"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Persona"/>
        <rdfs:comment xml:lang="es">Persona que recibe las enseñanzas y conocimientos de un académico en una institución de educación superior o centro de investigación.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Archivo -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Archivo">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#BitStream"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RepositorioInstitucional"/>
        <rdfs:isDefinedBy xml:lang="es">Recurso de información que se almacena en Dspace.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ArticuloCientifico -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ArticuloCientifico">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#PublicacionCientifica"/>
        <rdfs:comment xml:lang="es">Documento electrónico de divulgación científica en el cual se presentan los resultados, avances e innovaciones relacionadas a un área determinada del conocimiento.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ArticuloDivulgacion -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ArticuloDivulgacion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#PublicacionDivulgacionDifusion"/>
        <rdfs:comment xml:lang="es">Documento electrónico en el cual se presentan comentarios, crónicas, entrevistas, reseñas etc., relacionadas con un tópico o tema específico.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Asociacion -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Asociacion">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Consorcio"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Institucion"/>
        <rdfs:comment xml:lang="es">Grupo de personas relacionadas entre si para hacer cumplir un fin especifico.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#AutoArchivo -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#AutoArchivo">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Almacenamiento"/>
        <rdfs:isDefinedBy xml:lang="es">Práctica donde los investigadores depositan o archivan sus trabajos en un repositorio institucional.</rdfs:isDefinedBy>
        <rdfs:seeAlso rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RepositorioInstitucional"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Autor -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Autor">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Persona"/>
        <rdfs:comment xml:lang="es">El autor es aquella persona que crea una obra, sea artística, literaria o científica.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#BaseDatos -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#BaseDatos">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RecursoDeInformacion"/>
        <rdfs:comment xml:lang="es">Conjunto de documentos electrónicos asociados a un tópico o dominio, los cuales pueden ser usados/analizados con propósitos de trabajo o investigación.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#BitStream -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#BitStream">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RepositorioInstitucional"/>
        <rdfs:comment xml:lang="es">Recurso de información que se almacena en Dspace.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CONACYT -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CONACYT">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Consejos"/>
        <rdfs:comment>Es un organismo público descentralizado del gobierno federal mexicano dedicado a promover y estimular el desarrollo de la ciencia y la tecnología en ese país. Tiene la responsabilidad oficial para elaborar las políticas de ciencia y tecnología nacionales.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="es">Consejo Nacional de Ciencia y Tecnología</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CapituloDeLibroCientifico -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CapituloDeLibroCientifico">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#PublicacionCientifica"/>
        <rdfs:comment xml:lang="es">División que se hace en un libro de carácter científico  para facilitar la comprensión de un tema específico.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CapituloLibroDivulgacion -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CapituloLibroDivulgacion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#PublicacionDivulgacionDifusion"/>
        <rdfs:comment xml:lang="es">División que se hace en un libro de carácter de divulgación para facilitar la comprensión de un tema específico.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Catalogacion -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Catalogacion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Funcionalidad"/>
        <rdfs:comment xml:lang="es">Proceso de obtención o creación de metadatos que representan los recursos de información en el repositorio institucional.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CatalogoCONACYT -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CatalogoCONACYT">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Catalogacion"/>
        <rdfs:isDefinedBy xml:lang="es">Catálogo el cual contendrá la clasificación de áreas temáticas de ciencia y tecnología y los criterios mediante los cuales se homologan o normalizan los códigos de identificación para el autor, la institución y el área del conocimiento, disciplinas y subdisciplinas.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CentroDeInvestigacion -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CentroDeInvestigacion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Institucion"/>
        <rdfs:comment xml:lang="es">Organización cuyo propósito es el de generar conocimiento científico, tecnológico, social y humanístico relacionado a vareas áreas de investigación.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Coleccion -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Coleccion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Archivo"/>
        <rdfs:comment xml:lang="es">Conjunto de documentos electrónicos del mismo tipo agrupados en Dspace.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Comite -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Comite">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#GlosarioTerminos"/>
        <rdfs:isDefinedBy xml:lang="es">Comité de Acceso Abierto a la Información Científica, Tecnológica y de Innovación.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Comunidad -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Comunidad">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Archivo"/>
        <rdfs:isDefinedBy xml:lang="es">En Dspace, agrupa a una o más colecciones; puede tener un responsable distinto al del repositorio institucional.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Consejos -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Consejos">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Institucion"/>
        <rdfs:comment xml:lang="es">Organismo público creado para atender/satisfacer un fin relacionado a un área especifica (laboral, investigación, etc.).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Consorcio -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Consorcio">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Institucion"/>
        <rdfs:comment xml:lang="es">Grupo de personas relacionadas entre si para hacer cumplir un fin especifico.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Creacion -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Creacion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#PoliticaRepositorio"/>
        <rdfs:isDefinedBy xml:lang="es">Conjunto de objetivos, estrategias, así como líneas de acción que definen los mecanismos de creacion y enriquecimiento de la colección de datos en el repositorio institucional.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CreacionProceso -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CreacionProceso">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ProductoDelDesarrolloTecnologicoYLaInnovacion"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CreacionPrototipo -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CreacionPrototipo">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ProductoDelDesarrolloTecnologicoYLaInnovacion"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CreacionServicio -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CreacionServicio">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ProductoDelDesarrolloTecnologicoYLaInnovacion"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CreativeCommons -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CreativeCommons">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Licencia"/>
        <rdfs:isDefinedBy xml:lang="es">Conjunto de licencias públicas de libre difusión que complementan los derechos de autor y fomentan la colaboración, así como la distribución y uso de los materiales creativos.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CuerpoAcademico -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#CuerpoAcademico">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#InstitucionEducacionSuperior"/>
        <rdfs:comment>Grupo de profesores de tiempo completo (normalmente) que persiguen objetivos comunes en la generación del conocimiento o en la aplicación innovadora del mismo.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#DINI -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#DINI">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Identificador"/>
        <rdfs:isDefinedBy xml:lang="es">Iniciativa alemana para la información en red, por sus siglas en alemán, la cual contempla criterios para los repositorios, autores y editoriales, en cuanto a aspectos legales y seguridad de la información, entre otros.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#DatosDeInvestigaciones -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#DatosDeInvestigaciones">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RecursoDeInformacion"/>
        <rdfs:isDefinedBy xml:lang="es">Información recolectada y utilizada para la investigación académica, científica, tecnológica y de innovación.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Decanatura -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Decanatura">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#InstitucionEducacionSuperior"/>
        <rdfs:comment>Oficina responsable de la dirección académica y administrativa de una facultad universitaria.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#DesarrolloTecnologico -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#DesarrolloTecnologico">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ProductoDelDesarrolloTecnologicoYLaInnovacion"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#DiagnosticoDelEstadoDeLaTecnologia -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#DiagnosticoDelEstadoDeLaTecnologia">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ProductoDelDesarrolloTecnologicoYLaInnovacion"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Diplomado -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Diplomado">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#NivelAcademico"/>
        <rdfs:comment xml:lang="es">Curso de mediana o larga duración, el cual es generalmente dictado por una universidad o centro de investigación y tiene el propósito de enseñar, complementar o actualizar algún conocimiento o habilidad específica.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Director -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Director">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Administrativo"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Rector"/>
        <rdfs:comment xml:lang="es">Persona encargada de dirigir una actividad o proceso dentro de una empresa, institución de educación o centro de investigación.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Discente -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Discente">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Estudiante"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Persona"/>
        <rdfs:comment xml:lang="es">Persona que recibe las enseñanzas y conocimientos de un académico en una institución de educación superior o centro de investigación.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Diseminacion -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Diseminacion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Funcionalidad"/>
        <rdfs:isDefinedBy xml:lang="es">Transmisión de información académica, científica, tecnológica y de innovación desarrollada por parte de los investigadores o especialistas que utiliza un lenguaje especializado.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Distribucion -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Distribucion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#PoliticaRepositorio"/>
        <rdfs:comment xml:lang="es">Define los lineamientos para la divulgación y difusión del contenido del repositorio institucional.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="es">Una política de distribución se refiere a la forma o conjunto de criterios en la que se distribuyen los documentos y los metadatos del repositorio institucional. Indica qué pueden hacer los usuarios con los documentos o metadatos descargados en un marco legal.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Doctorado -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Doctorado">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#NivelAcademico"/>
        <rdfs:comment xml:lang="es">Es el grado más alto que confiere una institución educativa o centro de investigación en determinada disciplina y/o profesión, la cual certifica que una persona a dominado totalmente la teoria y practica asociada a un area de estudio.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#DublinCore -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#DublinCore">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Metadatos"/>
        <rdfs:isDefinedBy xml:lang="es">Estructura normada de metadatos enfocados a la descripción de recursos de información (electrónicos e impresos) de distintos dominios informativos.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Embargo -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Embargo">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#GlosarioTerminos"/>
        <rdfs:isDefinedBy xml:lang="es">Periodo de tiempo en el cual las editoriales retienen los derechos exclusivos de publicación del autor.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#EnmiendasDeAutor -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#EnmiendasDeAutor">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#GlosarioTerminos"/>
        <rdfs:isDefinedBy xml:lang="es">Modificaciones a los contratos, mediante las cuales se pueden modificar algunas cláusulas para retener derechos de autor.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Especialidad -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Especialidad">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#NivelAcademico"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#EstandaresInternacionales -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#EstandaresInternacionales">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#AccesoAbierto"/>
        <rdfs:isDefinedBy xml:lang="es">Lineamientos y procedimientos técnicos y administrativos establecidos por agencias internacionales de normalización de las que México forma parte para la organización, procesamiento, preservación, almacenamiento y recuperación de la información documental de referencia y texto completo; las características estructurales y la certificación, acreditación y evaluación de los repositorios.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Estudiante -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Estudiante">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Persona"/>
        <rdfs:comment xml:lang="es">Persona que recibe las enseñanzas y conocimientos de un académico en una institución de educación o centro de investigación.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#EvaluacionPorPares -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#EvaluacionPorPares">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#GlosarioTerminos"/>
        <rdfs:comment xml:lang="es">También conocido como arbitraje.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="es">Es el proceso por medio del cual se validan los materiales académicos, en donde autores de rango semejante o superior al del autor, revisan la calidad, originalidad, factibilidad y rigor científico de la investigación.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ExAlumno -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ExAlumno">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ExDiscente"/>
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ExEstudiante"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Persona"/>
        <rdfs:comment xml:lang="es">Persona que alguna vez perteneció a una institución de educación superior o centro de investigación y que además contribuyo con la creación de varios documentos electrónicos durante su estancia.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ExDiscente -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ExDiscente">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ExEstudiante"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Persona"/>
        <rdfs:comment xml:lang="es">Persona que alguna vez perteneció a una institución de educación superior o centro de investigación y que además contribuyo con la creación de varios documentos electrónicos durante su estancia.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ExEstudiante -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ExEstudiante">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Persona"/>
        <rdfs:comment xml:lang="es">Persona que alguna vez perteneció a una institución de educación superior o centro de investigación y que además contribuyo con la creación de varios documentos electrónicos durante su estancia.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Funcionalidad -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Funcionalidad">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RepositorioInstitucional"/>
        <rdfs:isDefinedBy xml:lang="es">Agrupa las tareas que realizan los usuarios del repositorio institucional.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#GlosarioTerminos -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#GlosarioTerminos">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RepositorioInstitucional"/>
        <rdfs:isDefinedBy xml:lang="es">Conjunto de definiciones de lineamientos técnicos que no se asocian con las clases principales del RI.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Handle -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Handle">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Metadatos"/>
        <rdfs:isDefinedBy xml:lang="es">Sistema de identificación y preservación de objetos digitales, el cual asigna una dirección permanente en internet, independientemente a la dirección del servidor donde éstos se encuentran alojados.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Identificador -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Identificador">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Coleccion"/>
        <rdfs:comment xml:lang="es">Código alfanumérico único utilizado por varios países para identificar de manera inequívoca a un repositorio.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Innovacion -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Innovacion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ProductoDelDesarrolloTecnologicoYLaInnovacion"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Institucion -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Institucion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RepositorioInstitucional"/>
        <rdfs:isDefinedBy xml:lang="es">Organismo público o privado fundado para desempeñar una labor científica, académica o de investigación.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#InstitucionEducacionSuperior -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#InstitucionEducacionSuperior">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Institucion"/>
        <rdfs:comment xml:lang="es">Organizaciones o entidades que cuentan con el reconocimiento oficial para prestar servicios de educación superior a un grupo de personas.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Interoperabilidad -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Interoperabilidad">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Funcionalidad"/>
        <rdfs:isDefinedBy xml:lang="es">Capacidad del Repositorio Nacional y de los Repositorios Institucionales de intercambiar información, acorde con parámetros que cumplan con los protocolos de metadatos para su recuperación.</rdfs:isDefinedBy>
        <rdfs:seeAlso rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RepositorioInstitucional"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#LA-Referencia -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#LA-Referencia">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Consorcio"/>
        <rdfs:isDefinedBy xml:lang="es">Cosechador latinoamericano de repositorios institucionales que se alimenta de los nodos nacionales.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#LibroCientifico -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#LibroCientifico">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#PublicacionCientifica"/>
        <rdfs:comment xml:lang="es">Documento de texto dividido en capítulos o secciones que incluyen un amplio compendio de información relacionada a un tópico o tema.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#LibroDivulgacion -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#LibroDivulgacion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#DublinCore"/>
        <rdfs:comment xml:lang="es">Documento de texto dividido en capítulos o secciones que incluyen información relacionada a un tópico o tema.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Licencia -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Licencia">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#AccesoAbierto"/>
        <rdfs:comment xml:lang="es">Una licencia es un contrato entre el licenciante (autor/titular de los derechos de explotación/distribuidor) y el licenciatario de un producto (usuario consumidor /usuario profesional o empresa), para utilizar dicho producto cumpliendo una serie de términos y condiciones establecidas.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Licenciatura -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Licenciatura">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#NivelAcademico"/>
        <rdfs:comment xml:lang="es">Título o grado académico que se obtiene tras realizar ciertos estudios de educación superior, los cuales certifican que una persona esta capacitada para cumplir con una serie de tareas en un ámbito laboral determinado.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Maestria -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Maestria">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#NivelAcademico"/>
        <rdfs:comment xml:lang="es">Grado obtenido después de hacer una licenciatura, el cual certifica que una persona esta capacitada para realizar una seria de tareas o enseñar la teoria relacionada a estas.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Mantenimiento -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Mantenimiento">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#PoliticaRepositorio"/>
        <rdfs:isDefinedBy xml:lang="es">Una política de mantenimiento hace referencia a la frecuencia en la que se propone realizar los respaldos del repositorio; indica quién es el responsable de hacer estos respaldos y el tipo de respaldo que se hará, el cual podría ser completo o incremental.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#MejoraAlEstadoDeLaTecnologia -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#MejoraAlEstadoDeLaTecnologia">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ProductoDelDesarrolloTecnologicoYLaInnovacion"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#MejoraProceso -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#MejoraProceso">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ProductoDelDesarrolloTecnologicoYLaInnovacion"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#MejoraPrototipo -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#MejoraPrototipo">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ProductoDelDesarrolloTecnologicoYLaInnovacion"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#MejoraServicio -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#MejoraServicio">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ProductoDelDesarrolloTecnologicoYLaInnovacion"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#MemoriasDeCongreso -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#MemoriasDeCongreso">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ArticuloCientifico"/>
        <rdfs:comment>Compendio de artículos de investigación derivados de un congreso, simposio o convención de divulgación científica.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Metadatos -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Metadatos">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Coleccion"/>
        <rdfs:isDefinedBy xml:lang="es">Elementos de descripción que proveen información básica de las publicaciones y demás recursos de información electrónicos.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#NivelAcademico -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#NivelAcademico">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RepositorioInstitucional"/>
        <rdfs:isDefinedBy xml:lang="es">Niveles o grados a obtener en instituciones de educación superior o centros de investigación.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#OAI-PMH -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#OAI-PMH">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Metadatos"/>
        <rdfs:comment xml:lang="en">Open Archives Initiative Protocol for Metadata Harvesting</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="es">Protocolo de la iniciativa de archivos abiertos para la cosecha de metadatos, por sus siglas en inglés, el cual es un mecanismo para la interoperabilidad de los repositorios.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ObjetosAprendizaje -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ObjetosAprendizaje">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RecursoDeInformacion"/>
        <rdfs:comment xml:lang="es">Un objeto de aprendizaje es un conjunto de recursos digitales, auto contenible y reutilizable, con un propósito educativo y constituido por al menos tres componentes internos: Contenidos, actividades de aprendizaje y elementos de contextualización.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#OpenAIRE -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#OpenAIRE">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#AccesoAbierto"/>
        <rdfs:comment xml:lang="en">Open Access Infrastructure for Research in Europe.</rdfs:comment>
        <rdfs:comment xml:lang="es">Puesta en marcha en la Universidad de Gante, Bélgica.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="es">Infraestructura de Acceso Abierto a la Investigación en Europa</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Patente -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Patente">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ProductoDelDesarrolloTecnologicoYLaInnovacion"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Persona -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Persona">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RepositorioInstitucional"/>
        <rdfs:isDefinedBy xml:lang="es">Es un individuo de la especie humana.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#PersonaExterna -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#PersonaExterna">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Persona"/>
        <rdfs:comment xml:lang="es">Académico, alumno u otra persona ajena a la universidad o centro de investigación donde se aloja el repositorio institucional el cual colaboro en la elaboración de uno o varios documentos electrónicos dentro del repositorio.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#PoliticaRepositorio -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#PoliticaRepositorio">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RepositorioInstitucional"/>
        <rdfs:isDefinedBy xml:lang="es">Conjunto de técnicas utilizadas para buscar, categorizar y acceder manera inequívoca, al texto completo de publicaciones reconocidas por los sectores académico, de ciencia, tecnología e innovación.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Preservacion -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Preservacion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Funcionalidad"/>
        <rdfs:isDefinedBy xml:lang="es">Mecanismos que garantizan la persistencia y permanencia de las colecciones en el repositorio institucional a través del tiempo tomando en cuenta los cambios tecnológicos.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ProductoDelDesarrolloTecnologicoYLaInnovacion -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ProductoDelDesarrolloTecnologicoYLaInnovacion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RecursoDeInformacion"/>
        <rdfs:comment xml:lang="es">Conjunto de acciones y/o actividades a realizarse dentro del repositorio para trabajar/manipular elementos electrónicos.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ProfesorDeTiempoCompleto -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ProfesorDeTiempoCompleto">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Academico"/>
        <rdfs:comment xml:lang="es">Docente que se dedica de tiempo completo a la enseñanza en una escuela o centro de investigación.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ProfesorTiempoParcial -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ProfesorTiempoParcial">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Academico"/>
        <rdfs:comment xml:lang="es">Docente que imparte uno o más cursos dentro de una universidad o centro de investigación pero solo por un periodo de tiempo determinado.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ProgramaAcademico -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ProgramaAcademico">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#InstitucionEducacionSuperior"/>
        <rdfs:comment>Diversas modalidades de planes de estudio que son ofrecidas por una universidad para la enseñanza de distintos tópicos relacionados a un área especifica.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#PublicacionCientifica -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#PublicacionCientifica">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RecursoDeInformacion"/>
        <rdfs:comment xml:lang="es">Obra impresa o electrónica en la que se promueve la investigación a través de la escritura de artículos científicos.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#PublicacionDivulgacionDifusion -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#PublicacionDivulgacionDifusion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RecursoDeInformacion"/>
        <rdfs:comment></rdfs:comment>
        <rdfs:comment xml:lang="es">Obra impresa o electrónica en la que se promueve la divulgación de información a través de la escritura de artículos.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Rector -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Rector">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Administrativo"/>
        <rdfs:comment xml:lang="es">Máxima autoridad académica de una universidad o centro de investigación.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Recuperacion -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Recuperacion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Funcionalidad"/>
        <rdfs:comment xml:lang="es">Mecanismos que permiten consultar, seleccionar y exportar documentos contenidos en las colecciones del repositorio institucional.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RecursoDeInformacion -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RecursoDeInformacion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Coleccion"/>
        <rdfs:comment xml:lang="es">Distintos tipos de elementos electrónicos disponibles para almacenarse en un repositorio.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Redalyc -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Redalyc">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#GlosarioTerminos"/>
        <rdfs:comment xml:lang="es">Base de datos en línea y repositorio digital de textos científicos en acceso abierto.</rdfs:comment>
        <rdfs:comment xml:lang="es">Red de Revistas Científicas de América Latina y el Caribe, España y Portugal</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ReporteDeInvestigacion -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ReporteDeInvestigacion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#PublicacionCientifica"/>
        <rdfs:comment xml:lang="es">Documento donde se presentan los experimentos y/o resultados de un estudio con respecto a un tema específico. Dicho estudio pretende dar respuesta a varias preguntas de investigación, las cuales pueden ampliar el conocimiento en un área.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Repositorio -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Repositorio">
        <rdfs:isDefinedBy xml:lang="es">Plataforma digital centralizada que, siguiendo estándates internacionales, almacena, mantiene, preserva y disemina la información científica, tecnológica y de innovación, la cual se deriva de las investigaciones, productos educativos, académicos y desarrollos tecnológicos.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RepositorioInstitucional -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RepositorioInstitucional">
        <rdfs:comment xml:lang="es">Plataforma digital centralizada que, siguiendo estándates internacionales, almacena, mantiene, preserva y disemina la información científica, tecnológica y de innovación, la cual se deriva de las investigaciones, productos educativos, académicos y desarrollos tecnológicos.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="es">Plataforma digital de las instituciones de los sectores social, privado y gubernamental, que contiene su información académica, científica, tecnológica y de innovación, la cual deberá vincularse con el Repositorio Nacional siguiendo estándares internacionales previstos en estos Lineamientos Técnicos.</rdfs:isDefinedBy>
        <rdfs:seeAlso rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#EstadaresInternacionales"/>
        <rdfs:seeAlso rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RepositorioNacional"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RepositorioNacional -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RepositorioNacional">
        <rdfs:isDefinedBy xml:lang="es">Repositorio Nacional de Acceso Abierto a Recursos de Información Académica, Científica, Tecnológica y de Innovación, de calidad e interés social y cultural el cual consiste en una plataforma digital centralizada cuya coordinación y modelos de operación serán emitidos por el CONACYT, que siguiendo estándares internacionales, almacena, mantiene, preserva y disemina la información académica, científica, tecnológica y de innovación, la cual se deriva de las investigaciones, productos educativos y académicos.</rdfs:isDefinedBy>
        <rdfs:seeAlso rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RecursoDeInformacionAcademicaCientificaTecnologicaYDeInnovacion"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ResearchGate -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ResearchGate">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#GlosarioTerminos"/>
        <rdfs:isDefinedBy xml:lang="es">Red social de investigadores que comparten producción científica para el avance del conocimiento.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ResponsableRI -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ResponsableRI">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#Persona"/>
        <rdfs:comment xml:lang="es">Responsable de controlar y/o supervisar el buen funcionamiento del repositorio institucional.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RevistaCientificaDeAccesoAbierto -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RevistaCientificaDeAccesoAbierto">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ArticuloCientifico"/>
        <rdfs:comment>Publicación periódica de acceso gratuito en la que se describen los avances relacionados a la ciencia y tecnología a través de la difusión de distintos artículos.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RevistaCientificaDeAccesoRestringido -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RevistaCientificaDeAccesoRestringido">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ArticuloCientifico"/>
        <rdfs:comment>Publicación periódica de acceso restringido en la que se recoge el progreso de la ciencia y tecnología a través de la divulgación de distintos artículos.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RevistaDeAccesoAbierto -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RevistaDeAccesoAbierto">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ArticuloDivulgacion"/>
        <rdfs:comment>Publicación periódica de acceso gratuito en la que se describen o discuten distintos temas a través de la difusión de artículos.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RevistaDeAccesoRestringido -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RevistaDeAccesoRestringido">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#ArticuloDivulgacion"/>
        <rdfs:comment>Publicación periódica de acceso restringido en la que se habla o comenta acerca de varios temas a través de la divulgación de artículos.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#SIICYT -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#SIICYT">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#GlosarioTerminos"/>
        <rdfs:isDefinedBy xml:lang="es">Sistema Integrado de Información sobre Investigación Científica, Desarrollo Tecnológico e Innovación.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#SNCTI -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#SNCTI">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#GlosarioTerminos"/>
        <rdfs:isDefinedBy xml:lang="es">Sistema Nacional de Ciencia, Tecnología e Innovación.</rdfs:isDefinedBy>
