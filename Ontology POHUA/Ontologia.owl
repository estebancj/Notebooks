    <!-- http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RepositorioUDLAP -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/ontologies/2017/0/repositorioUDLAP.owl#RepositorioUDLAP">
